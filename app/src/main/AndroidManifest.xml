        <activity
            android:name="com.dev.exch22.Activity.Term_and_condition_Activity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.dev.exch22.Activity.Contact_us_Activity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.dev.exch22.MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.dev.exch22.Activity.How_to_playMainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.dev.exch22.Activity.About_Activity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.dev.exch22.Activity.FeaturesActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.dev.exch22.Activity.Splash_Activity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.dev.exch22.Activity.LegalityActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.dev.exch22.Activity.Withdraw_Terms_Activity"
            android:screenOrientation="portrait" />
        <activity android:name="com.facebook.ads.AudienceNetworkActivity" android:hardwareAccelerated="true"/>
    </application>

</manifest>